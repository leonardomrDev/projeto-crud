{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/front-end/src/App.js"
      },

      {
        "source": "**",
        "destination": "/front-end/src/index.js"
      }
    ]
  }
}
